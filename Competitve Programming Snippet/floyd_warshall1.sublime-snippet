	<content><![CDATA[

// Floyd Warshall

#define inf 1e12
const ll N = 509;
vector<vl> gr(N, vl(N, inf));
ll n, m;

void floyd_warshall1()
{
    for (ll i = 1; i <= n; i++)
        gr[i][i] = 0;

    for (ll k = 1; k <= n; k++)
    {
        for (ll i = 1; i <= n; i++)
        {
            for (ll j = 1; j <= n; j++)
            {
                if (gr[i][k] < inf && gr[k][j] < inf)
                    gr[i][j] = min(gr[i][j], gr[i][k] + gr[k][j]);
            }
        }
    }
}


